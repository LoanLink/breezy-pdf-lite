      "generator": "secret"
    },
    "NODE_ENV": {
      "description": "node env",
      "value": "production"
    }
  },
  "addons": [
  ],
  "image": "heroku/nodejs",
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-google-chrome"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs"
    }
  ]
}
